{
    "label": "Cloud Backups",
    "launch": "launch.sh",
    "description": "Pak to to back up saves, screenshots and other folders to cloud storage providers like Google Drive and Dropbox.",
    "platforms": [
        "miyoomini",
        "my282",
        "rg35xxplus",
        "tg5040"
    ],
    "settings": {
        "source_folders": [
            "/Saves",
            "/Screenshots"
        ],
        "rclone_destination": "/MinUI-Cloud-Backups",
        "rclone_backup_prefix": "Backup",
        "rclone_remote_name": "trimui"
    }
}
